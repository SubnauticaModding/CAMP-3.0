{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "LICENSE": true,
    ".gitignore": true,
    "pnpm-lock.yaml": true,
    "pnpm-debug.log": true,
    "package-lock.json": true,
    "node_modules": true,
    "tsconfig.json": true,
    ".vscode": true,
    "tslint.json": true,
    "dist/[abcefghijklmnopqrstuvwxyz]*": true,
    "dist/d[bcdefghijklmnopqrstuvwxyz]*": true,
    "dist/da[abcdefghijklmnopqrsuvwxyz]*": true,
    "dist/dat[bcdefghijklmnopqrstuvwxyz]*": true
